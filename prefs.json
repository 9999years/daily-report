{
	"format": [
		"         good morning!",
		"{today()}",
		"{align(now_hm(), '', iso_date())}", "{hrule()}",
		"{forecast()!c}",
		"{conditions()!c}",
		"{sun()}",
		"{moon()!c}", "{thinhrule()}",
		"tomorrow: {tmrw_forecast()}",
		"{tmrw_conditions()!c}", "{hrule()}",
		"{weather_graph()}", "{hrule()}",
		"{work()}", "{thinhrule()}",
		"{todo()}", "{thinhrule()}",
		"{calendar()}", "{thinhrule()}",
		"{countdown()}",
		"\n\n\n\n"
	],
	"google_key_path": "google_keys.json",
	"google_credential_path": "google_credentials.json",
	"app_name": "the daily report",
	"api_keys": "keys.json",
	"width": 32,
	"horiz": "═",
	"horiz_light": "─",
	"vert": "║",
	"vert_light": "│",
	"weather": {
		"location": "va/reston",
		"height": 6,
		"chars": {
			"precip": "·",
			"temp":   "×",
			"both":   "¤"
		},
		"max_retries": 3,
		"forecast_format": "{low}-{high}°F, {precip}%p",
		"moon": {
			"bright_graphic": "○",
			"dark_graphic": "●",
			"format": "{graphic} {phase} @ {percent}%"
		},
		"sunrise_format": "{sunrise} ↑",
		"sunset_format": "{sunset} ↓",
		"daylight_format": "{daylight}",
		"sun_format": "{align(sunrise, daylight, sunset)}",
		"temp": "fahrenheit"
	},
	"calendar": {
		"scope": "https://www.googleapis.com/auth/calendar.readonly",
		"todo_pat": "to.?do",
		"countdown_pat": "count.?downs?",
		"work_pat": ".*work.*",
		"todo_check": "[] ",
		"max_countdowns": 10
	},
	"maze": {
		"height": 10,
		"style": "random",
		"halves": false,
		"start": "S",
		"end": "E"
	}
}
