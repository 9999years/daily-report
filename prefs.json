{
	"format": [
		"good morning!",
		"{today}",
		"{iso_date}", "{hrule}",
		"{forecast}",
		"{conditions}",
		"{sun}",
		"{moon}", "{thinhrule}",
		"{tmrw_forecast}",
		"{tmrw_conditions}", "{hrule}",
		"{weather_graph}", "{hrule}",
		"{work}", "{thinhrule}",
		"{todo}", "{thinhrule}",
		"{calendar}", "{thinhrule}",
		"{countdown}",
		"\n\n\n"
	],
	"google_key_path": "google_keys.json",
	"google_credential_path": "google_credentials.json",
	"app_name": "the daily report",
	"api_keys": "keys.json",
	"width": 32,
	"horiz": "═",
	"horiz_light": "─",
	"vert": "║",
	"vert_light": "│",
	"weather": {
		"location": "va/reston",
		"height": 6,
		"chars": {
			"precip": "·",
			"temp":   "×",
			"both":   "¤"
		},
		"max_retries": 3,
		"today_prefix": "",
		"tomorrow_prefix": "tomorrow: ",
		"moon": {
			"bright_graphic": "○",
			"dark_graphic": "●",
			"format": "{graphic} {phase} @ {percent}%"
		},
		"sunrise_suffix": " ↑",
		"sunset_suffix": " ↓",
		"daylight_suffix": "",
		"forecast_format": "{prefix}{low}-{high}°F, {precip}%p",
		"temp": "fahrenheit"
	},
	"calendar": {
		"scope": "https://www.googleapis.com/auth/calendar.readonly",
		"todo_pat": "to.?do",
		"countdown_pat": "count.?downs?",
		"work_pat": ".*work.*",
		"todo_check": "[] ",
		"max_countdowns": 10
	},
	"maze": {
		"height": 10,
		"style": "random",
		"halves": false,
		"start": "S",
		"end": "E"
	}
}
